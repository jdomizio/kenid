{
  "name": "ken-id",
  "version": "1.0.0",
  "description": "An application to help scan potentially fraudulent licenses",
  "main": "index.js",
  "author": "Jason Domizio",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "base64-font-loader": "0.0.4",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.2",
    "electron": "^1.6.8",
    "electron-builder": "^18.0.1",
    "electron-rebuild": "^1.5.11",
    "eslint": "^3.19.0",
    "file-loader": "^0.11.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.10.2",
    "normalize.css": "^7.0.0",
    "postcss-loader": "^2.0.5",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.3",
    "redux-form": "^6.7.0",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.5",
    "scss-loader": "0.0.1",
    "style-loader": "^0.18.1",
    "webpack": "^2.6.0"
  },
  "dependencies": {
    "@jdomizio/license-scanner": "^1.0.3",
    "classnames": "^2.2.5",
    "csv": "^1.1.1",
    "electron-debug": "^1.1.0",
    "moment": "^2.18.1",
    "pouchdb-node": "^6.2.0",
    "winston": "^2.3.1",
    "winston-daily-rotate-file": "^1.4.6"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "start npm run watch && start npm run start:dev",
    "prod": "start npm run watch && start npm run start:production",
    "start": "npm run start:dev",
    "start:dev": "cross-env NODE_ENV=dev electron .",
    "start:production": "cross-env NODE_ENV=production electron .",
    "watch": "cross-env NODE_ENV=dev webpack --watch",
    "build": "npm run rebuild-bindings && npm run build:ui",
    "build:ui": "cross-env NODE_ENV=production webpack --bail",
    "rebuild-bindings": "npm run rebuild-bindings:leveldown",
    "rebuild-bindings:leveldown": "electron-rebuild -f -w leveldown",
    "electron:v": "electron -v",
    "clean:lib": "rimraf lib",
    "clean:dist": "rimraf dist",
    "clean:db": "rimraf default-db",
    "clean": "npm run clean:lib && npm run clean:dist && npm run clean:db",
    "pack": "build --dir",
    "dist": "build"
  },
  "build": {
    "appId": "com.jdomizio.kenid",
    "productName": "kenid",
    "icon": "letter-k-256.ico",
    "files": [
      "**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme,test,__tests__,tests,powered-test,example,examples,*.d.ts}",
      "!**/node_modules/.bin",
      "!**/*.{o,hprof,orig,pyc,pyo,rbc}",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,__pycache__,thumbs.db,.gitignore,.gitattributes,.editorconfig,.flowconfig,.yarn-metadata.json,.idea,appveyor.yml,.travis.yml,circle.yml,npm-debug.log,.nyc_output,yarn.lock,.yarn-integrity}",
      "!default-db/**/*",
      "!src/ui/**/*"
    ]
  },
  "postinstall": "install-app-deps"
}
